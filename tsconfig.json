{
    "compilerOptions": {
        "module": "none",
        "target": "es5",
        "rootDir": "src",
        "outDir": "dist",
        "noImplicitAny": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "sourceMap": false
    },
    "exclude": [
        "node_modules",
        "typings"
    ]
}
